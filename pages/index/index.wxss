.container{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 80rpx;
}
.container map{
  width: 100%;
  height: 100%;
}
.btns{
  position: absolute;
  bottom: 0;
  left: 0 ;
  display: flex;
  background: #03bbd5;
  width: 100%;
}
.btns view{
  flex:1;
  text-align: center;
  line-height: 80rpx;
  height: 80rpx;
  color: #fff;

}
.btns view:nth-of-type(1){
  background: #ff9700;
}